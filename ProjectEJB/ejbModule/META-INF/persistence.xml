<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_2.xsd">
	<persistence-unit name="projectEJB" transaction-type="JTA">
		<jta-data-source>ProjectDB</jta-data-source>
		<class>model.Answer</class>
		<class>model.AnswerPK</class>
		<class>model.Blacklist</class>
		<class>model.Question</class>
		<class>model.Questionnaire</class>
		<class>model.Review</class>
		<class>model.User</class>
		<properties>
			<property name="eclipselink.logging.level" value="FINE" />
			<property name="hibernate.jdbc.time_zone" value="UTC"/>
		</properties>
	</persistence-unit>
</persistence>
